<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape>
            <solid
                android:color="@android:color/transparent"/>

            <stroke
                android:color="@color/white"
                android:dashWidth="1dp"
                android:dashGap="0dp"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>

